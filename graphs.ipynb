   "cell_type": "code",
   "source": [
    "import networkx as nx\n",
    "from pyvis import network as net\n",
    "\n",
    "g = net.Network(notebook=True, cdn_resources=\"remote\", width=\"100%\", select_menu=True)\n",
    "\n",
    "nxg = nx.random_geometric_graph(20, 0.525, seed=666)\n",
    "g.from_nx(nxg)\n",
    "g.show_buttons()\n",
    "g.show(\"graph.html\")\n",
    "\n"
   ],
